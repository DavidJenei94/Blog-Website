<%- include("partials/header") %>
    <h1>Home</h1>
    <p><%= content %></p>

    <% posts.forEach(element => { %>
        <h4><%= element.title %></h4>
        <p>
            <%= element.text.substring(0,100) + " ... " %>
            <a href="/posts/ <%= element._id %>">Read more</a>
        </p>
    <% }); %>

<%- include("partials/footer") %>